    <link href=<%= asset_path("favicon.ico")%> rel="shortcut icon">
  </head>
  <body>
    <%#= render 'shared/navbar' %>
    <%#= render 'shared/alerts' %>
    <%#= yield %>
    <%#= render 'shared/footer' %>
    <div class="container">
    
    
      <!-- Example row of columns -->
      <div class="row row-centered">
        <div class="col-md-12 col-lg-12 col-sm-12 col-centered">
          
            
            <div class="info">
              <div class="logo"></div>
                
                <div class="divisor"></div>
                
                <div class="pronto">MUY PRONTO...</div>
                
            </div>
          
        </div>
        
        
      
      <footer>
        
        <a class="facebook" href="https://www.facebook.com/comotti" target="_self"></a>
        
      </footer>

       
    </div> <!-- /container -->  
    
     
    
    
    
    
  

